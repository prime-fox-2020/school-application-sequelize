<%- include ('partials/header', {title: 'List of students'})%>
    <%- include ('partials/middle')%>
    <table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">First_Name</th>
      <th scope="col">Last_Name</th>
      <th scope="col">Email</th>
      <th scope="col">Gender</th>
      <th scope="col">Birth_date</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <% for(let i = 0; i < data.length; i++) {%>
    <tr>
      <th ><%=data[i].id%></th>
      <th ><%=data[i].first_name%></th>
      <th ><%=data[i].last_name%></th>
      <th ><%=data[i].email%></th>
      <th ><%=data[i].gender%></th>
      <th ><%=data[i].birthdate%></th>
      <th> <a href='/students/<%= data[i].id %>/edit'> Edit</a></th> %>
     <th> <a href='/students/<%= data[i].id %>/delete'> Delete</a></th> %> 
    </tr>
  <% } %>
  <a href="/students/add">Add Student!</a>
    <%- include ('partials/footer')%>
