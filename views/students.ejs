<%- include ('./partials/header', {title: 'List of Students'}) %>
    <table>
        <tr>
            <th>id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Gender</th>
            <th>Date of Birth</th>
            
        </tr>
        <% for (let i = 0; i< data.length; i++) { %>
            <tr>
                <td><%= data[i].id %></td>
                <td><%= data[i].first_name %></td>
                <td><%= data[i].last_name %></td>
                <td><%= data[i].email %></td>
                <td><%= data[i].gender %></td>
                <td><%= data[i].birth_date %></td>
                <td><a href="/students/<%= data[i].id %>/edit">Edit</a></td>
                <td><a href="/students/<%= data[i].id %>/delete">Delete</a></td>
            </tr>
        <% } %>
    </table>
    <a href="/students/add_student" class="link">Add Student</a>
</body>
</html>