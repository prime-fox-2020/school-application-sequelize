<%- include('./partial/header') %>
<body>
  <%- include('./partial/navbar') %>
  <h1 align="center">Subjects Data</h1><br><br><br><br>
  <% if (data.length) { %>
    <table class="table table-sm table-dark">
      <thead>
        <tr>
          <th scope="col">ID</th>
          <th scope="col">Subject Name</th>
        </tr>
    </thead>
    <tbody>
      <% data.forEach(list => { %>
        <tr>
          <td><%= list.id %></td>
          <td><%= list.subjectName %></td>
      <% }) %>
    </tbody>
  </table>
  <% }else{ %>
    <div class="alert alert-dark" role="alert">
      Subject not found!!
    </div>
    <%}%>
    </tbody>
  </table>
</body>
</html>