<%- include('./partials/head')%>
<style>
  form {
    margin: 0 auto;
    width:250px;
    display: flex;
    flex-flow: row;
  }
  </style>

<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Subject Name</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i = 0; i < subjects.length;i++){%>
      <tr>
        <td><%= subjects[i].id %></td>
        <td><%= subjects[i].subject_name %></td>
      </tr>
      <% } %>
  </tbody>
</table>


<%- include('./partials/foot')%>