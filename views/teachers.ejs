<!DOCTYPE html>
<head>
    <title>Teacher's List</title>
</head>

<body>
    <h1> Teachers's List</h1>
        <table border = 1 >
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>E-mail</th>
        </tr>
        <% for(var i = 0; i< data.length; i++) {%>
        <tr>
            <td><%= data[i].id %></td>
            <td><%= data[i].first_name %></td>
            <td><%= data[i].last_name %></td>
            <td><%= data[i].email %></td>
        </tr>

        <% } %>

    </table>
</body>