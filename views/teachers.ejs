<%- include ('./partials/header', {title: 'List of Teachers'}) %>
    <table>
        <tr>
            <th>id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Gender</th>
        </tr>
        <% for (let i = 0; i< data.length; i++) { %>
            <tr>
                <td><%= data[i].id %></td>
                <td><%= data[i].first_name %></td>
                <td><%= data[i].last_name %></td>
                <td><%= data[i].email %></td>
                <td><%= data[i].gender %></td>
            </tr>
        <% } %>
    </table>
</body>
</html>